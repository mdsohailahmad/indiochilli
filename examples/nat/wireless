config wifi-device 'radio0'
        option type 'mac80211'
        option path 'pci0000:00/0000:00:00.0'
        option band '5g'
        option htmode 'VHT80'
        option disabled '0'
        option ucentral_path '/radios/1'
        option channel '52'
        option txantenna '1'
        option rxantenna '1'
        option beacon_int '100'
        option dtim_period '2'
        option country 'IN'
        option legacy_rates '0'
        option acs_exclude_dfs '0'
        list channels '36'
        list channels '44'
        list channels '52'
        list channels '60'
        list channels '100'
        list channels '108'
        list channels '116'
        list channels '124'
        list channels '132'
        list channels '140'
        list channels '149'
        list channels '157'
        list channels '165'
        list channels '173'
        list channels '184'
        list channels '192'

config wifi-device 'radio1'
        option type 'mac80211'
        option path 'platform/ahb/18100000.wmac'
        option band '2g'
        option htmode 'HT20'
        option disabled '0'
        option ucentral_path '/radios/0'
        option channel '11'
        option txantenna '3'
        option rxantenna '3'
        option beacon_int '100'
        option dtim_period '2'
        option country 'IN'
        option legacy_rates '0'
        option acs_exclude_dfs '0'

config wifi-iface 'down1v0_0_0'
        option ucentral_path '/interfaces/1/ssids/0'
        option device 'radio1'
        option network 'down1v0'
        option ssid 'T-Guest'
        option mode 'ap'
        option wds '0'
        option wpa_disable_eapol_key_retries '0'
        option disassoc_low_ack '0'
        option ieee80211w '2'
        option encryption 'psk2'
        option key '1234567890'
        option proxy_arp '1'
        option hidden '0'
        option time_advertisement '0'
        option isolate '0'
        option uapsd '0'
        option multicast_to_unicast '0'
        option ifname 'wlan-hs24'

config wifi-vlan
        option iface 'down1v0_0_0'
        option name 'v#'
        option vid '*'

