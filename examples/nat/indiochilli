        option enable_chilli_kname '1'
        option chilli_bridge_mode '0'
        option bridge_mode_without_vlan '0'
        option wan_dev 'eth1'
        option lan_dev 'eth0'
        option hs_uplink_dev 'eth1'
        option configured '1'
        option hs_uplink_interface 'up0v0'
        option hs_downlink_interface 'down1v0'
